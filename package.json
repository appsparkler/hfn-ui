{
  "name": "@appsparkler/hfn-ui",
  "version": "0.1.11",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "browser": "dist/hfn-ui.min.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@hfn-checkins/types": "^1.1.3",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.3",
    "@reduxjs/toolkit": "^1.8.1",
    "@sekmet/react-confetti": "^6.0.1",
    "@zxing/library": "^0.19.1",
    "firebase": "^9.8.4",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.8",
    "redux": "^4.1.2",
    "redux-first-router": "^2.1.5",
    "redux-thunk": "^2.4.1",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:serve": "yarn build && yarn serve build -l 5000",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "compile": "rimraf dist && yarn rollup -c",
    "deploy": "yarn build && npx netlify deploy -d build",
    "deploy-prod": "yarn deploy --prod",
    "lint": "yarn eslint --ext .ts,.tsx src",
    "typecheck": "tsc --project ./tsconfig.json --noEmit --skipLibCheck"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@storybook/addon-actions": "^6.5.0-beta.1",
    "@storybook/addon-essentials": "^6.5.0-beta.1",
    "@storybook/addon-interactions": "^6.5.0-beta.1",
    "@storybook/addon-links": "^6.5.0-beta.1",
    "@storybook/builder-webpack5": "^6.5.0-beta.1",
    "@storybook/manager-webpack5": "^6.5.0-beta.1",
    "@storybook/node-logger": "^6.5.0-beta.1",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.5.0-beta.1",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.180",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@types/react-redux": "^7.1.23",
    "@types/redux": "^3.6.0",
    "@types/redux-first-router": "^2.1.8",
    "@types/uuid": "^8.3.4",
    "netlify-cli": "^10.1.0",
    "query-string": "^7.1.1",
    "react-scripts": "5.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "serve": "^14.0.1",
    "tsc-alias": "^1.6.5",
    "typescript": "^4.6.2",
    "web-vitals": "^2.1.4",
    "webpack": "^5.70.0"
  }
}
